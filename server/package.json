{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "e2e:db": "dotenv -e .env.e2e -- prisma migrate deploy && dotenv -e .env.e2e -- prisma db seed",
    "start:e2e": "dotenv -e .env.e2e -- tsx src/server.ts",
    "test:db": "dotenv -e .env.test -- prisma migrate deploy",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "dev": "nodemon --exec tsx src/server.ts",
    "prebuild": "prisma generate",
    "start": "node dist/server.js",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "prisma": {
    "seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@prisma/client": "^6.12.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^10.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4"
  }
}
